leetcode 

brute : gives tle
tc-o(n2)
sc-o(1)

class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector<int> ans(n); //giving size is very imp otherwise we can get run time error
        for(int i=0;i<n;i++){
            int pro=1;
            for(int j=0;j<n;j++){
                
                if(i!=j){
                    pro*=nums[j];
                }
            }
            ans[i]=pro;
        }
        return ans;
    }
};

better: //we need suffix and prefix array , 
ans[i]=product before i * product after i
tc-o(n)
sc-O(2n)=o(n) //we are not counting ans array
class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector<int>  pre(n);
        vector<int> suff(n);
        vector<int> ans(n);

        pre[0]=1;
        for(int i=1;i<n;i++){
            pre[i]=pre[i-1]*nums[i-1];
        }
        suff[n-1]=1;
        for(int i=n-2;i>=0;i--){
            suff[i]=suff[i+1]*nums[i+1];
        }

        for(int i=0;i<n;i++){
            ans[i]=pre[i]*suff[i];
        }
        return ans;
    }
};


optimal ; we dont need prefix and suffix array , we can store it in ans array
tc-o(n)
sc-o(1) //we are not considering ans array.
class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector<int> ans(n,1);
        int pre=1;
        for(int i=0;i<n;i++){
             ans[i]*=pre;
             pre*=nums[i] ;//update prefix
        }

        int suff=1;
        for(int i=n-1;i>=0;i--){
            ans[i]*=suff;
            suff*=nums[i];
        }

        return ans;
    }
};
